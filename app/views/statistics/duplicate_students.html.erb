<h1>Duplicate Students Across Different Classes</h1>

<table>
  <thead>
    <tr>
      <th>Student Name</th>
      <th>Number of Classes</th>
    </tr>
  </thead>
  <tbody>
    <% @duplicate_students.each do |id, name, class_count| %>
      <tr>
        <td><%= link_to name, student_path(id) %></td>
        <td><%= class_count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br> </br>
<%= link_to 'Back to Statistics', statistics_path %>
