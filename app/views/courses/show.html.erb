<%= render @course %>
<div class="button-container">
  <%= link_to "Edit this course", edit_course_path(@course), class: "btn btn-maroon" %>
  <%= button_to "Destroy this course", @course, method: :delete, class: "btn btn-maroon", data: { confirm: "Are you sure?" } %>
  <%= link_to "Back to Courses", courses_path, class: "btn btn-maroon" %>

</div>

